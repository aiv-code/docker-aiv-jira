<?xml version="1.0" encoding="UTF-8" standalone="no"?><DATASOURCE name="Jira" sourcetype="jdbc" type="postgresql"><DATASET isStoredProc="false" name="Issue by priority" type="NormalDataset"><QUERY>select count(i.key) as total_issue, p.name, s.name as statusName, ip.name as issue_priority from issues i
