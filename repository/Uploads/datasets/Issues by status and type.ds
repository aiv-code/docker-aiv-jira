<?xml version="1.0" encoding="UTF-8" standalone="no"?><DATASOURCE name="Jira" sourcetype="jdbc" type="postgresql"><DATASET isStoredProc="false" name="Issues by status and type" type="NormalDataset"><QUERY>select count(i.key) as total_issue, p.name, i.issuestatus, s.name as statusName,
