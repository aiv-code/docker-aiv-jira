{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": ["null", "string"],
      "set":"primary"
    },
    "branches": {
      "type": ["null", "array"],
      "updatable": "true",
      "items": {
        "type": ["null", "object"]
      }
    },
    "pullRequests": {
      "type": ["null", "array"],
      "updatable": "true",
      "items": {
        "type": ["null", "object"]
      }
    },
    "repositories": {
      "type": ["null", "array"],
      "updatable": "true",
      "items": {
        "type": ["null", "object"]
      }
    },
    "_instance": {
      "type": ["null", "object"],
      "updatable": "true",
      "properties": {
