{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "set": "primary"
    },
    "self": {
      "type": "string",
      "updatable": "true"
    },
    "state": {
      "type": "string",
      "updatable": "true"
    },
    "name": {
      "type": "string",
      "updatable": "true"
    },
    "startDate": {
      "type": "string",
      "updatable": "true",
      "format": "date-time"
    },
    "endDate": {
      "type": "string",
      "updatable": "true",
      "format": "date-time"
    },
    "completeDate": {
      "type": "string",
      "updatable": "true",
      "format": "date-time"
    },
    "originBoardId": {
      "type": "integer",
      "updatable": "true"
    },
    "goal": {
      "type": "string",
      "updatable": "true"
    }
  }
}
